{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "no-inline-styles": "off",
    "button-type": "off",
    "axe/text-alternatives": [
      "default",
      {
        "document-title": "off",
        "image-alt": "off",
        "frame-title": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
    ],
    "disown-opener": "off",
    "axe/parsing": [
      "default",
      {
        "duplicate-id-aria": "off",
        "duplicate-id-active": "off"
      }
    ],
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "backface-visibility",
          "-webkit-text-size-adjust",
          "-ms-touch-action",
          "scrollbar-width",
          "overflow-anchor"
        ]
      }
    ],
    "highest-available-document-mode": "off",
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-required-children": "off",
        "aria-required-parent": "off"
      }
    ]
  }
}